{
	"expo": {
		"name": "simple-todo-app",
		"description": "Simple Todo List Application for Productivity.",
		"slug": "simple-todo-app",
		"privacy": "public",
		"sdkVersion": "30.0.0",
		"platforms": ["ios", "android"],
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#d13e60"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"androidStatusBar": {
			"backgroundColor": "#d13e60"
		}
	}
}
